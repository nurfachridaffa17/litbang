<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="rengar_program_form_view" model="ir.ui.view">
            <field name="name">rengar_program_form_view</field>
            <field name="model">rengar.program</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_rengar_program">
            <field name="name">Master Program</field>
            <field name="res_model">rengar.program</field>
            <field name="view_mode">tree,form</field>
            <field name="view_type">form</field>
            <field name="domain">[]</field>
        </record>
    </data>
</odoo>