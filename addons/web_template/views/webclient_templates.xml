<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <template id='web_login' inherit_id="web.login">
            <xpath expr="//t/form/div[@class='form-group field-login']" position="replace">
                <div class="form-group field-login">
                    <label for="login" class="control-label">NRP</label>
                    <input type="text" name="login" t-att-value="login" id="login" class="form-control" required="required" autofocus="autofocus" autocapitalize="off"/>
                </div>
            </xpath>
        </template>
    
    </data>

</odoo>
