<odoo>
	<data>
		<record model="ir.ui.view" id="jurnal_view_form_id">
			<field name="name">surat keluar form</field>
			<field name="model">jurnal</field>
			<field name="arch" type="xml">
				<form string="jurnal Form">
				<sheet>
					<div class="oe_title">
						<label for="name" class="oe_edit_only" string="Judul Jurnal" />
						<h2>
							<field name="name"/> 
						</h2>
					</div>
					<group>
						<field name="url_file" />
					</group>
					<group >				
						<field name="ket" cols="20" rows="10"/>
					</group>
					
				</sheet>
				</form>
			</field>
		</record>


		<!--  ________ berita view tree _______-->
	<record model="ir.ui.view" id="jurnal_view_tree_id">
	    <field name="name">jurnal.list</field>
	    <field name="model">jurnal</field>
	    <field name="arch" type="xml">
	    <!-- Konten: tp_arch_*_component (tree) -->
			<tree string="berita list">
   			 	<field name="name"/>
			</tree>
	    </field> <!-- end arch -->
	</record>
	
	<!--  ________ berita view search _______-->
	<record model="ir.ui.view" id="jurnal_view_search_id">
	    <field name="name">jurnal.search</field>
	    <field name="model">jurnal</field>
	    <field name="arch" type="xml">
	    <!-- Konten: tp_arch_*_component (search) -->
			<search>
    			<field name="name"/> 
			</search>
	    </field> <!-- end arch -->
	</record>

	</data>
</odoo>