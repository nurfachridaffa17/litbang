<odoo>
  <data>
	<!--  ________ master_flow view form _______-->
	<record model="ir.ui.view" id="master_flow_view_form_id">
	    <field name="name">master_flow.form</field>
	    <field name="model">rikwastu.master_flow</field>
	    <field name="arch" type="xml">
	   	<!-- Konten: tp_arch_*_component (form) -->
			<form string="master_flow Form">
			    <header>
					<!-- tp_button_workflow_db, tp_button_workflow_engine disini -->
					<!-- tp_button_smart_button disini -->
			    </header>
            	<sheet>
              		<div class="oe_title">
         				<label for="name" class="oe_edit_only" string="Nama" />
         				<h1><field name="name"/></h1>
         			</div>
 						
                  <group>
                  		<!-- field lainnya disini -->
                  		<field name="urutan"/>
                  		<field name="step_id"/>
						<field name="deskripsi"/>
						<field name="next_step"/>
						<field name="active"/>
                  </group>
              	</sheet>
        	</form>
	    </field> <!-- end arch -->
	</record>
	
	<!--  ________ master_flow view tree _______-->
	<record model="ir.ui.view" id="master_flow_view_tree_id">
	    <field name="name">master_flow.list</field>
	    <field name="model">rikwastu.master_flow</field>
	    <field name="arch" type="xml">
	    <!-- Konten: tp_arch_*_component (tree) -->
			<tree string="master_flow list">
			    <field name="name"/>
			    <field name="deskripsi"/>
			</tree>
	    </field> <!-- end arch -->
	</record>
	
	<!--  ________ master_flow view search _______-->
	<record model="ir.ui.view" id="master_flow_view_search_id">
	    <field name="name">master_flow.search</field>
	    <field name="model">rikwastu.master_flow</field>
	    <field name="arch" type="xml">
	    <!-- Konten: tp_arch_*_component (search) -->
			<search>
    			<field name="name"/>
    			<field name="deskripsi"/>
			</search>
	    </field> <!-- end arch -->
	</record>
	
  </data>	
</odoo>