		<!-- __________ MENU rikwastu __________ -->
		<!-- __Menu-Top__ -->
		<menuitem name="Verifica Audit" id="rikwastu_menu_root" /> <!-- sequence="10" -->
		<!-- __Menu-Level_1__ (categories) diperlukan di Left Bar Menu -->

		<menuitem name="Perencanaan" parent="rikwastu_menu_root" id="rikwastu_menu_1"
			sequence="1" /> <!-- sequence="10" -->
		<menuitem name="Judul Penelitian" parent="rikwastu_menu_1"
			id="rikwastu_menu_1_1" action="act_Program" sequence="2" /> <!-- sequence="10" -->
		<menuitem name="Proses Perencanaan" parent="rikwastu_menu_1"
			id="rikwastu_menu_1_2" action="act_perencanaan" sequence="3" />

		<menuitem name="Pelaksanaan" parent="rikwastu_menu_root" id="rikwastu_menu_2"
			sequence="4" /> <!-- sequence="10" -->
		<menuitem name="Managemen Kegiatan" parent="rikwastu_menu_2"
			id="rikwastu_menu_2_1" action="perencanaan_tahunan_action_window_id"
			sequence="5" /> <!-- sequence="10" -->
		<menuitem name="Proses Pelaksanaan" parent="rikwastu_menu_2"
			id="rikwastu_menu_2_2" action="pelaksanaan_action_window_id"
			sequence="6" /> <!-- sequence="10" -->

		<!-- __Menu-Level_2__ (Call Actions) -->
		<menuitem name="Finalisasi" parent="rikwastu_menu_root" id="rikwastu_menu_3"
			sequence="7" />
		<menuitem name="Proses Finalisasi" parent="rikwastu_menu_3"
			id="rikwastu_menu_3_1" action="finalisasi_action_window_id" sequence="8" /> <!-- sequence="10" -->

		<!-- __Menu-Level_2__ (Call Actions) -->
		<menuitem name="Configurasi" parent="rikwastu_menu_root" id="rikwastu_menu_4"
			sequence="10" />
		<menuitem name="Master Flow" parent="rikwastu_menu_4" id="rikwastu_menu_4_1"
			action="master_flow_action_window_id" sequence="11" /> <!-- sequence="10" -->
		<menuitem name="Template Surat" parent="rikwastu_menu_4" id="rikwastu_menu_4_2"
			action="act_template" sequence="12" />
		<menuitem name="Tujuan Surat" parent="rikwastu_menu_4" id="rikwastu_menu_4_3"
			action="act_tujuansrt" sequence="13" />
		<menuitem name="Step Kegiatan" parent="rikwastu_menu_4" id="rikwastu_menu_4_4"
			action="act_kegiatan" sequence="14" />


	</data>
</odoo>
