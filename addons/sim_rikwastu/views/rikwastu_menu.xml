		<record model="ir.actions.act_window" id="act_Program">
			<field name="name">Judul Penelitian</field>
			<field name="res_model">program</field>
			<field name="target">current</field>
			<field name="context">{}</field> <!-- {'state': 'open'} -->
			<field name="domain">[]</field>
			<field name="limit">80</field> <!-- max 80 baris dlm 1 page -->
			<field name="view_type">form</field>
			<field name="view_mode">tree,form,graph,kanban</field>
			<!-- pilihan view_mode: tree,form,graph,calendar,kanban,pivot,gantt -->
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Tambahkan Data</p>
			</field>
		</record>

