			</field>
		</record>
		<!-- bayu end -->

		<!-- __________ ACTION perencanaan_tahunan ___________ -->
		<!-- Action harus dideklarasi sebelum menu -->
		<record id="perencanaan_tahunan_action_window_id" model="ir.actions.act_window">
			<field name="name">Setting Pelaksanaan</field> <!-- Nama action ini muncul di breadcrump -->
			<field name="res_model">rikwastu.perencanaan_tahunan</field>
			<field name="target">current</field>
