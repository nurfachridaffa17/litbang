			</field>
		</record>

		<!-- __________ ACTION master_flow ___________ -->
		<!-- Action harus dideklarasi sebelum menu -->
		<record id="master_flow_action_window_id" model="ir.actions.act_window">
			<field name="name">Master Flow</field> <!-- Nama action ini muncul di breadcrump -->
			<field name="res_model">rikwastu.master_flow</field>
			<field name="target">current</field>
