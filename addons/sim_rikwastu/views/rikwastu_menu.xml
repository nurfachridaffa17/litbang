			<field name="limit">80</field>  <!-- max 80 baris dlm 1 page -->
			<field name="view_type">form</field>
			<field name="view_mode">kanban,tree,form</field>
			<!-- pilihan view_mode: tree,form,graph,calendar,kanban,pivot,gantt -->
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Tambahkan data pertama</p>
			</field>
		</record>

		
 

		<!-- __________ ACTION pelaksanaan ___________ -->
		<!-- Action harus dideklarasi sebelum menu -->
