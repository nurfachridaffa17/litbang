<odoo>
	<data>
		<record id="view_rekayasa_teknologi_tree" model="ir.ui.view">
            <field name="name">view_rekayasa_teknologi_tree</field>
            <field name="model">survey.inovasi.rekayasa.teknologi</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="spesifikasi_teknis"/>
                </tree>
            </field>
        </record>

        <record id="view_rekayasa_teknologi_form" model="ir.ui.view">
            <field name="name">view_rekayasa_teknologi_form</field>
            <field name="model">survey.inovasi.rekayasa.teknologi</field>
            <field name="arch" type="xml">
                <form>
                	<header>
                	</header>
                	<sheet>
                		<group>
                			<field name="name"/>
                            <field name="spesifikasi_teknis"/>
                		</group>
                		<group>
                			<field name="foto" widget="image"/>
                		</group>
                	</sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_ujicoba_rekayasa_teknologi_form">
            <field name="name">Inovasi dan Rekayasa Teknologi</field>
            <field name="res_model">survey.inovasi.rekayasa.teknologi</field>
            <field name="view_type">form</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Click disini untuk menambahkan hasil survey rekayasa teknologi</p>
            </field>
        </record>
	</data>
</odoo>