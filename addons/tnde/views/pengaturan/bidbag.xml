<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<!--  ________ pengaturan_disposisi view tree _______-->
		<record model="ir.ui.view" id="bidbag_view_tree_id">
		    <field name="name">bidbag.list</field>
		    <field name="model">bidbag</field>
		    <field name="arch" type="xml">
		    <!-- Konten: tp_arch_*_component (tree) -->
				<tree string="bidbag list">
				    <field name="name"/>
				</tree>
		    </field> <!-- end arch -->
		</record>
		
			    
		<record model="ir.ui.view" id="bidbag_view_form_id">
			<field name="name">bidbag.form</field>
			<field name="model">bidbag</field>
			<field name="arch" type="xml">
				<form string="bidbag Form">
					<header>
						<!-- Tambah Surat Masuk -->
					</header>
				<sheet>
					<div class="oe_title">
						<label for="name" class="oe_edit_only" string="Kode Klasifikasi" />
						<h2>
							<field name="name" />
						</h2>
					</div>
				</sheet>
				</form>
			</field>
		</record>
	
	
		<record model="ir.ui.view" id="bidbag_view_search_id">
			<field name="name">bidbag.search</field>
			<field name="model">bidbag</field>
			<field name="arch" type="xml">
				<!-- Konten: tp_arch_*_component (search) -->
				<search>
						<field name="name" />
				</search>
			</field> <!-- end arch -->
		</record>
	</data>
</odoo>