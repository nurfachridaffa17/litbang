<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<!--  ________ pengaturan_disposisi view tree _______-->
		<record model="ir.ui.view" id="kode_tersier_view_tree_id">
		    <field name="name">kode_tersier.list</field>
		    <field name="model">kode_tersier</field>
		    <field name="arch" type="xml">
		    <!-- Konten: tp_arch_*_component (tree) -->
				<tree string="kode_tersier list">
				    <field name="name"/>
	    			<field name="nm_kode"/>
	    			<field name="kode_klasifikasi"/>
	    			<field name="kode_primer"/>
    				<field name="kode_sekunder"/>
				</tree>
		    </field> <!-- end arch -->
		</record>
		
			    
		<record model="ir.ui.view" id="kode_tersier_view_form_id">
			<field name="name">kode_tersier.form</field>
			<field name="model">kode_tersier</field>
			<field name="arch" type="xml">
				<form string="kode_tersier Form">
					<header>
						<!-- Tambah Surat Masuk -->
					</header>
				<sheet>
					<div class="oe_title">
						<label for="name" class="oe_edit_only" string="Kode Klasifikasi" />
						<h2>
							<field name="name" />
						</h2>
					</div>
					<group>
	    				<field name="nm_kode"/>
		    			<field name="kode_klasifikasi"/>
		    			<field name="kode_primer"/>
		    			<!-- <field name="kode_primer"  domain="([('kode_klasifikasi','==',kode_klasifikasi)])" /> -->
	    				<field name="kode_sekunder" />
	    				<!-- <field name="kode_sekunder"  domain="([('kode_primer','==',kode_primer)])" /> -->
					</group>
				</sheet>
				</form>
			</field>
		</record>
	
	
		<record model="ir.ui.view" id="kode_tersier_view_search_id">
			<field name="name">kode_tersier.search</field>
			<field name="model">kode_tersier</field>
			<field name="arch" type="xml">
				<!-- Konten: tp_arch_*_component (search) -->
				<search>
						<field name="name" />
		    			<field name="nm_kode"/>
		    			<field name="kode_klasifikasi"/>
		    			<field name="kode_primer"/>
	    				<field name="kode_sekunder"/>
				</search>
			</field> <!-- end arch -->
		</record>
	</data>
</odoo>