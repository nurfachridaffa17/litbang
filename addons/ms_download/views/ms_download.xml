<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="ms_word_survey_form" model="ir.ui.view">
            <field name="name">ms.word.survey.form.inherit</field>
            <field name="model">survey.survey</field>
            <field name="inherit_id" ref="survey.survey_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="download_word" string="Download Word" type="object" class="oe_highlight"/>
                    <button name="download_excel" string="Download Excel" type="object" class="oe_highlight"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>